{
    "name": "local",
    "host": "127.0.0.1",
    "protocol": "sftp",
    "port": 2222,
    "username": "vagrant",
    "privateKeyPath": "~/.ssh/insecure_private_key",
    "remotePath": "/var/www/html/ignitionGo",
    "uploadOnSave": true,
    "watch": {
        "files": false,
        "autoUpload": true,
        "autoDelete": true
    },
    "ignore:": [
        "**/.vagrant/**",
        "**/vagrantfiles/**",
        "**/Vagrantfile/**",
        "**/.vscode/**",
        "**/.git/**",
        "**/docker/**",
        "**/.dockerignore",
        "**/docker-compose.yml",
        "**/Dockerfile",
        "**/*.log"
    ]
}
